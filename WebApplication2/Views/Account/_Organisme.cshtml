@model CommercentViewModel




<div class="row">
    <h5 class="info-text"> Document et Numéro fiscale </h5>
    <div class="col-sm-4 col-sm-offset-1">
        <div class="form-group">
            <div class="custom-file">
                <label class="custom-file-label">Veulleir Telecharger un copie de  la patente<small>(PDF)</small>...</label>
                <input asp-for="Documents" id="PATENTEUPLOAD" multiple class="form-control custom-file-input">
            </div>

        </div>
        <div class="form-group">
            <div class="custom-file">
                <label class="custom-file-label">Veulleir Telecharger un copie de  la RNE</label>
                <input asp-for="Documents" id="RNEUPLOAD" multiple class="form-control custom-file-input">
            </div>

        </div>
        
    </div>

    <div class="col-sm-6">
        <div class="form-group">
            <label>Documents a Télécharger</label>
            <p class="description" id="docs">
                <b>-Patente.</b><br />
                <b>-RNE</b><br />
                <b id="writing"></b>
                <h5> -Un administrateur Va vérifier votre inscription.</h5>
            </p>
        </div>
    </div>


</div>

