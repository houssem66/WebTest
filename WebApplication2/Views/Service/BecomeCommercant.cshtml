                        }
                        else
                        {
                            switch ($(this).val())                        
                            {
                                case "TransportReguliers":
                                    document.getElementById("optionalLabel").innerHTML = $docAlc + 'CAD transport réguliers' + '</label>';
                                    document.getElementById("writing").innerHTML = ('<b>-CAD transport réguliers<b>');
                                    break;
                                case "Transport publique non réguliers":
