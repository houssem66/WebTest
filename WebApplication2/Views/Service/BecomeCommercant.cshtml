    <script>
        $(document).ready(function () {
            $("[name=jobb]").on('change', function () {
                var value = $("input[name='jobb']:checked").val();
                if (value == "Nourriture") {
                    $("#secteur option[value='Mobility']").hide();
                    $("#secteur option[value='FoodTech']").show();
                    $("#secteur option[value='TravelTech']").show();
                    $("#secteur option[value='RealEstate Tech']").hide();
                    $("#secteur option[value='Autre']").show();
                    document.getElementById("demo").innerHTML = "Bonjour nous allons vous aider à devenir un commercant (Nourriture)!";
                }
                else if (value == "Logement") {

                    $("#secteur option[value='Mobility']").hide();
                    $("#secteur option[value='FoodTech']").hide();
                    $("#secteur option[value='TravelTech']").show();
                    $("#secteur option[value='RealEstate Tech']").show();
                    $("#secteur option[value='Autre']").show();
                    document.getElementById("demo").innerHTML = "Bonjour nous allons vous aider à devenir un commercant (Logement)!";
                }
                else {

                    $("#secteur option[value='Mobility']").show();
                    $("#secteur option[value='FoodTech']").hide();
                    $("#secteur option[value='TravelTech']").show();
                    $("#secteur option[value='RealEstate Tech']").hide();
                    $("#secteur option[value='Autre']").show();
                    document.getElementById("demo").innerHTML = "Bonjour nous allons vous aider à devenir un commercant (Transport)!";

                }



            });
            $("#secteur option[value='Mobility']").show();
            $("#secteur option[value='FoodTech']").show();
            $("#secteur option[value='TravelTech']").show();
            $("#secteur option[value='RealEstate Tech']").show();
            $("#secteur option[value='Autre']").show();
        });

    </script>
    @*afficher le drop list secteur avec le valuer du service*@

    <script>
