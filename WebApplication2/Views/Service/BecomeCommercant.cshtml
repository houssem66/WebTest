                                    document.getElementById("writing").innerHTML = ('<b>-CAD transport non réguliers<b>');
                                    break;
                                case "Tranposrt touristique":
                                    document.getElementById("optionalLabel").innerHTML = $docAlc + 'CAD transport touristique' + '</label>';
                                    document.getElementById("writing").innerHTML = ('<b>-CAD transport touristique<b>');
                                    break;
                                default: 
                                    document.getElementById("optionalLabel").innerHTML = $docAlc + 'CAD transport privé' + '</label>';
                                    document.getElementById("writing").innerHTML = ('<b>-CAD transport privé<b>');
                            } 
                            

                        }
                    }              
            }
               
            });
