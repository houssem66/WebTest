                    {
                        $("#optionalFile").show();
                        $("#writing").show();

                        if ($(this).val() == "Débit de boissons alcoolisées") {
                            document.getElementById("optionalLabel").innerHTML = $docAlc + 'Veulleir Telecharger une copie de la Licence pour débits de boissons de deuxième et troisième catégorie et Restaurants avec boissons alcoolisées' + '</label>';
