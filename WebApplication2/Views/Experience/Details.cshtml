    <section>
        <div class="rows inn-page-bg com-colo">
            <div class="container inn-page-con-bg tb-space">
                <div class="col-md-9">
                    <!--====== TOUR TITLE ==========-->

                    <div class="tour_head">
                        <h2>
                            @Model.Titre
                            <span class="tour_star">
                                @{ int i = 1;
                                    decimal s = ViewBag.avg;
                                    for (i = 1; i <= ViewBag.avg; i++)
                                    {
                                        <i class="fa fa-star" aria-hidden="true">
                                        </i> }
                                    s = Math.Abs((s - i));
                                    if ((s < 1) && (s > 0))
                                    {<i class="fa fa-star-half-o" aria-hidden="true">
                                        </i> } }
                            </span><span class="tour_rat">@ViewBag.avg</span>
                        </h2>

                    </div>
                    <!--====== TOUR DESCRIPTION ==========-->
                    <div class="tour_head1 hotel-com-color">
                        <h3>About @Model.Titre</h3>
                        @{ foreach (var item in Model.Activites)
                            {


                                var photoPath = "~/images/" + item.Image;
                                <p>@item.Image.ToString()  </p>
                            }
                        }

                    </div>
                    <!--====== ROOMS: HOTEL BOOKING ==========-->
                    <div class="tour_head1 hotel-book-room">
                        <h3>Photo Gallery</h3>
                        <div id="myCarousel1" class="carousel slide" data-ride="carousel">
                            <!-- Indicators -->
                            <ol class="carousel-indicators carousel-indicators-1">
                                @{int x = 0;
                                    foreach (var item in Model.Activites)
                                    {
                                        <li data-target="#myCarousel1" data-slide-to="@x">
                                            <img src="~/images/@item.Image" alt="Chania"><h3>@item.Details</h3>
                                        </li>

                                        x++;
                                      
                                    }


                                }
                            </ol>

                                <!-- Wrapper for slides -->
                            <div class="carousel-inner carousel-inner1" role="listbox">
                                @{ if (Model.Activites.Count() == 1)
                                    {
                                        foreach (var item in Model.Activites)
                                        {<div></div>
                                            <div class="item active"> <img src="~/images/@item.Image" alt="Chania" width="460" height="345"><h3>@item.Details</h3> </div>

                                        }
                                    }
                                    else
                                    {
                                        int j = 1;
                                        foreach (var item in Model.Activites)
                                        {

